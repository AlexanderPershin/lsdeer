{
  "name": "lsdeer",
  "version": "0.1.6",
  "private": true,
  "main": "index.js",
  "homepage": ".",
  "author": {
    "name": "Alexander Pershin"
  },
  "description": "lsdeer - file manager",
  "repository": "https://github.com/AlexanderPershin/lsdeer",
  "build": {
    "extends": null,
    "appId": "com.electron.lsdeer",
    "productName": "lsdeer",
    "copyright": "Copyright Â© 2020 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "publish": [
        "github"
      ]
    },
    "files": [
      "./index.js",
      "./imageServer.js",
      "./assets/*",
      "./helpersMain/*",
      "./modulesMain/*",
      "./build/**/*",
      "./node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@electron/get": "^1.10.0",
    "@fluentui/react": "^7.106.4",
    "@react-hook/mouse-position": "^3.2.2",
    "@seznam/compose-react-refs": "^1.0.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@uifabric/file-type-icons": "^7.2.31",
    "bytes": "^3.1.0",
    "chokidar": "^3.4.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "electron-devtools-installer": "^3.1.0",
    "electron-is-dev": "^1.2.0",
    "electron-progressbar": "^1.2.0",
    "electron-react-devtools": "^0.5.3",
    "electron-redux-devtools": "^0.3.4",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.3.4",
    "electronbar": "^1.3.4",
    "express": "^4.17.1",
    "fix-path": "^3.0.0",
    "get-image-colors": "^2.0.1",
    "hex-and-rgba": "^1.4.2",
    "image-thumbnail": "^1.0.8",
    "nanoid": "^3.0.2",
    "natural-compare": "^1.4.0",
    "ncp": "^2.0.0",
    "rc-input-number": "^4.6.2",
    "react": "^16.13.1",
    "react-color": "^2.18.1",
    "react-contextmenu": "^2.13.0",
    "react-dom": "^16.13.1",
    "react-file-icon": "^0.2.0",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.0",
    "styled-spinkit": "^1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "set ELECTRON_DISABLE_SECURITY_WARNINGS=true && set ELECTRON_START_URL=http://localhost:3000/ && electron .",
    "dev": "concurrently -k \"npm start\" \"wait-on http://localhost:3000 && npm run electron-dev\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder -w ",
    "release": "electron-builder --win --publish=always"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "devtron": "^1.4.0",
    "electron": "^18.3.7",
    "electron-builder": "^22.7.0",
    "wait-on": "^4.0.1"
  }
}
